import matplotlib.pyplot as plt

# the given data
bots = ['Aggressive', 'Passive']
win_rates_randbot = [53.3, 57.3]

# Plot the graph
fig, ax = plt.subplots()

# the bar chart for Randbot
ax.bar(bots, win_rates_randbot, color=['lightgreen', 'lightgreen'])

# Adding labels and legend
ax.set_ylabel('Win Rate (%)')
ax.set_title('Win Rates of Randbot Against Aggressive and Passive Bots')

# Saving the plot as a PNG file
plt.savefig('win_rates_randbot_chart.png')

# Display the plot
plt.show()
